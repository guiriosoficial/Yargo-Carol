    "jquery",
    "minimalist",
    "parallax",
    "responsive",
    "rustic",
    "template",
    "website",
    "wedding"
  ],
  "description": "A responsive wedding website template with a minimalists and rustics design including a beautiful parallax and a simple fly-in effect.",
  "repository": {
    "type": "git",
    "url": "https://github.com/guiriosoficial/Yargo-Carol"
  },
  "main": "app/index.html",
  "private": true,
  "scripts": {
    "serve:test": "cross-env NODE_ENV=test gulp serve",
    "serve:dist": "cross-env NODE_ENV=production gulp serve",
    "start": "gulp serve",
    "build": "cross-env NODE_ENV=production gulp",
    "test": "npm run serve:test",
    "tasks": "gulp --tasks"
  },
  "dependencies": {
    "jquery": "~3.6.0",
    "jquery-mask-plugin": "~1.14.16"
  },
  "devDependencies": {
    "@babel/core": "~7.17.0",
    "@babel/preset-env": "~7.16.11",
    "autoprefixer": "~10.4.2",
    "browser-sync": "~2.27.10",
    "cross-env": "~7.0.3",
    "cssnano": "~5.0.16",
    "del": "~6.0.0",
    "gulp": "~4.0.2",
    "gulp-babel": "~8.0.0",
    "gulp-cli": "~2.3.0",
    "gulp-eslint": "~6.0.0",
    "gulp-htmlmin": "~5.0.1",
    "gulp-if": "~3.0.0",
    "gulp-imagemin": "~7.1.0",
    "gulp-load-plugins": "~2.0.7",
    "gulp-plumber": "~1.2.1",
    "gulp-postcss": "~9.0.1",
    "gulp-sass": "~5.1.0",
    "gulp-sourcemaps": "~3.0.0",
    "gulp-uglify": "~3.0.2",
    "gulp-useref": "~5.0.0",
    "sass": "~1.49.7"
  },
  "engines": {
    "node": "^16.13.1",
    "npm": "^8.1.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR",
    "not ie <= 8"
  ],
  "eslintConfig": {
    "parserOptions": {
      "sourceType": "module"
